  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5B5F71"
      }
    },
    {
      "name": "At Rules",
      "scope": ["keyword.control.at-rule"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5CF8C9"
      }
    },
    {
      "name": "Tag Name, Punctuation Tag",
      "scope": ["entity.name.tag","punctuation.definition.tag"],
      "settings": {
        "foreground": "#FE4980"
      }
    },
    {
      "name": "Class Selectors",
      "scope": ["entity.other.attribute-name.class", "punctuation.definition.entity"],
      "settings": {
        "foreground": "#FF6F7C"
      }
    },
    {
      "name": "ID Selectors",
      "scope": ["entity.other.attribute-name.id"],
      "settings": {
        "foreground": "#82BBEA"
      }
    },
    {
      "name": "Pseudo Element Selectors, Pseudo Class Selectors, Units",
      "scope": ["entity.other.attribute-name.pseudo-element","entity.other.attribute-name.pseudo-class", "keyword.other.unit"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#F66D9B"
      }
    },
    {
      "name": "Important Keyword",
      "scope": ["keyword.other.important"],
      "settings": {
        "foreground": "#FFED4A"
      }
    },
    {
      "name": "Property Name",
      "scope": ["support.type.property-name", "meta.property-name"],
      "settings": {
        "foreground": "#C4C4FF"
      }
    },
    {
      "name": "Atrribute Name",
      "scope": ["entity.other.attribute-name", "meta.attribute-selector"],
      "settings": {
        "foreground": "#75BEFF"
      }
    },
    {
      "name": "Property Value",
      "scope": ["support.constant.property-value", "meta.property-value"],
      "settings": {
        "foreground": "#E6E6E6"
      }
    },
    {
      "name": "Support Class",
      "scope": ["support.class", "entity.name.type.namespace"],
      "settings": {
        "foreground": "#4BC9B0"
      }
    },
    {
      "name": "Numeric | Constant Value",
      "scope": ["constant.numeric"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF6F7C"
      }
    },
    {
      "name": "Constant Other",
      "scope": ["constant.other"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#E2C196"
      }
    },
    {
      "name": "Support Function",
      "scope": ["support.function"],
      "settings": {
        "foreground": "#5CF8C9"
      }
    },
    {
      "name": "Single Quote, Double Quote",
      "scope": ["string.quoted.double","string.quoted.single"],
      "settings": {
        "foreground": "#C1E8CF"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#8FE4DC"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#88F7DE"
      }
    },
    {
      "name": "PHP Tags",
      "scope": ["punctuation.section.embedded"],
      "settings": {
        "foreground": "#9691FB"
      }
    },
    {
      "name": "PHP Construct",
      "scope": ["support.function.construct.output"],
      "settings": {
        "foreground": "#FE4980"
      }
    },
    {
      "name": "Storage Type, Storage Others",
      "scope": ["storage.type", "keyword", "keyword.other.phpdoc"],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#ED9EF8"
      }
    },
    {
      "name": "Storage Modifier",
      "scope": ["storage.modifier"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#84B7F4"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#53AFF4"
      }
    },
    {
      "name": "Keyword Control ,Keyword Other",
      "scope": ["keyword.control, keyword.other"],
      "settings": {
        "foreground": "#C4C4FF"
      }
    },
    {
      "name": "Variable Other",
      "scope": ["variable.other","punctuation.definition.variable"],
      "settings": {
        "foreground": "#F98487"
      }
    },
    {
      "name": "Variable This",
      "scope": ["variable.language.this"],
      "settings": {
        "foreground": "#F984CD"
      }
    },
    {
      "name": "Italics",
      "scope": ["variable.language.this"],
      "settings": {
        "fontStyle":"italic"
      }
    }
  ]
}
